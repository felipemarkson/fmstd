{
    "configurations": [
        {
            "name": "Linux",
            "intelliSenseMode": "linux-clang-x64",
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c99",
            "compilerArgs": [
                "-std=c99",
                "-pedantic",
                "-Wall",
                "-Wextra",
                "-Werror",
                "-ggdb",
                "-I."
            ],
            "includePath": [
                "${default}",
                "${workspaceFolder}"
            ],
            "defines": ["FM_TEST"]
        }
    ],
    "version": 4
}